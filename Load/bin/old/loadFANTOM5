ga GUS::Community::Plugin::InsertSegmentResult --extDbRlsSpec 'FANTOM5 Human Transcribed Enhancer Atlas: Predefined Enhancers|2015-03-18' --inFile ${FILE_DIR}/neuron_specific.txt --name 'Specifically expressed enhancers in neurons' --description 'predefined transcribed enhancers (FANTOM5) specifically expressed in neurons' --type 'OBI:0000905' --subtype 'NO:0000006' --sourceId ${SOURCE_ID}_NEURON_SPECIFIC --characteristicFile neuron_characteristics.txt   --sequenceExtDbRlsSpec 'GRC Human Reference Genome Assembly|GRCh37.p13/hg19' --commit > fantom5_neurons_specific.log 2>&1


