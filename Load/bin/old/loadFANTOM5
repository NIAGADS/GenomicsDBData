#ga GUS::Community::Plugin::InsertSegmentResult --extDbRlsSpec 'FANTOM5 Human Transcribed Enhancer Atlas: Predefined Enhancers|2015-03-18' --inFile ${FILE_DIR}/neuronal_stem_cell_expressed.txt --name 'Expressed enhancers in neuronal stem cells' --description 'predefined transcribed enhancers (FANTOM5) expressed in neuronal stem cells' --type 'OBI:0000905' --subtype 'NO:0000005' --sourceId ${SOURCE_ID}_NEURONAL_STEM_CELL --characteristicFile neuronal_stem_cell_characteristics.txt   --sequenceExtDbRlsSpec 'GRC Human Reference Genome Assembly|GRCh37.p13/hg19' --commit > fantom5_neuronal_stem_cell.log 2>&1

#ga GUS::Community::Plugin::InsertSegmentResult --extDbRlsSpec 'FANTOM5 Human Transcribed Enhancer Atlas: Predefined Enhancers|2015-03-18' --inFile ${FILE_DIR}/neuronal_stem_cell_specific.txt --name 'Specifically expressed enhancers in neuronal stem cells' --description 'predefined transcribed enhancers (FANTOM5) specifically expressed in neuronal stem cells' --type 'OBI:0000905' --subtype 'NO:0000006' --sourceId ${SOURCE_ID}_NEURONAL_STEM_CELL_SPECIFIC --characteristicFile neuronal_stem_cell_characteristics.txt   --sequenceExtDbRlsSpec 'GRC Human Reference Genome Assembly|GRCh37.p13/hg19' --commit > fantom5_neuronal_stem_cell_specific.log 2>&1

