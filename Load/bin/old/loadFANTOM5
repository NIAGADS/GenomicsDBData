#!/bin/bash

FILE_DIR=/mnt/data/GenomicsDB/functional_genomics/FANTOM5/predefined_transcribed_enhancers
VERSION=2015-03-18
DATABASE="FANTOM5 Human Transcribed Enhancer Atlas: Predefined Enhancers"
SOURCE_ID=FANTOM5

#ga GUS::Supported::Plugin::InsertExternalDatabase --name 'FANTOM5 Human Transcribed Enhancer Atlas: Predefined Enhancers' --commit
#ga GUS::Supported::Plugin::InsertExternalDatabaseRls --databaseName 'FANTOM5 Human Transcribed Enhancer Atlas: Predefined Enhancers' --databaseVersion $VERSION --commit

# neuronal stem cells
# generateSegmentResultLoadFile -i ${FILE_DIR}/CL:0000047_neuronal_stem_cell_expressed_enhancers.bed -q 0 -s 1 -e 2 -r 4 > ${FILE_DIR}/neuronal_stem_cell_expressed.txt
# generateSegmentResultLoadFile -i ${FILE_DIR}/CL:0000047_neuronal_stem_cell_differentially_expressed_enhancers.bed -q 0 -s 1 -e 2 -r 4 > ${FILE_DIR}/neuronal_stem_cell_specific.txt

