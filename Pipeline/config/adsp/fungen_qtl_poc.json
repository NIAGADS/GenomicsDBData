                    "genomeBuild": "GRCh38",
                    "seqrepoProxyPath": "@file.seqrepo_proxy_path",
                    "rankingFile": "@file.consequence_ranking_file",
                    "commitAfter": 100,
                    "log2stderr": true,
                    "logExisting": true
                }
            }
        },
