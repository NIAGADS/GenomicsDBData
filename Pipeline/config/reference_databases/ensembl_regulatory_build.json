{
    "file": {
        "path": "$DATA_DIR/Ensembl",
        "file": "Homo_sapiens.GRCh38.regulatory_activity.v112.tsv.gz"
    },
    "external_database": {
        "name": "Ensembl Regulatory Build",
        "description": "Ensembl Regulatory Build annotations",
        "version": "112",
        "release_date": "2024-02-14",
        "id_url": "https://www.ensembl.org/info/genome/funcgen/index.html",
        "download_url": "ftp://ftp.ensembl.org/pub/current_regulation/homo_sapiens/GRCh38/annotation/",
        "accession": "ENSEMBL_RB",
        "type": "Functional genomics"
    },
    "preprocess": [
        {
            "comment": "not really preprocessing but necessary to do it independently",
            "plugin": {
                "name": "GenomicsDBData::Load::Plugin::InsertStudy",
                "parameters": {
                    "name": "Ensembl Regulatory Build: Motifs",
                    "description": "The Ensembl Regulatory Build contains a genome-wide set of regions that are likely to be involved in gene regulation.  These include: enhancer, promoter, and open chromatin regions, as well as CTCF and transcription factor binding sites.",
                    "sourceId": "@external_database.accession",
                    "extDbRlsSpec": "@external_database.name|@external_database.version"
                }
            }
        }
    ],
    "load": [
        {
            "plugin": {
                "name": "GenomicsDBData::Load::Plugin::LoadEnsemblMotifFeatures",
                "parameters": {
                    "extDbRlsSpec": "@external_database.name|@external_database.version",
                    "fileDir": "@file.path",
                    "filePattern": "@file.pattern",
                    "schema": "results"
                }
            }
        }
    ]
}