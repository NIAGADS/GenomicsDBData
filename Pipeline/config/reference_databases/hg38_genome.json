{
    "file": {
	"path": "$DATA_DIR/GENCODE",
	"file": "GRCh38.p10.genome.fa.gz"
    },
    "external_database": {
	"name": "GENCODE",
	"version": "27/GRCh38.p10",
	"release_date": "2017-08-01",
	"download_url": "ftp://ftp.ebi.ac.uk/pub/databases/gencode/Gencode_human/release_27/",
	"id_url": "https://www.gencodegenes.org/human/release_27.html",
	"description": "nucleotide sequence of the GRCh38.p10 human genome assembly version on all regions, including reference chromosomes, scaffolds, assembly patches and haplotypes",
	"accession": "GENCODE_GRCH38",
	"type": "DNA sequence"
    },
    "load" : [
	{
	    "plugin": {
		"name": "GUS::Supported::Plugin::LoadFastaSequences",
		"parameters": {
		    "externalDatabaseName": "@external_database.name",
		    "externalDatabaseVersion": "@external_database.version",
		    "ncbiTaxonName": "Homo sapiens",
		    "tableName": "DoTS::ExternalNASequence",
		    "regexSourceId": ">(\\S+)",
		    "sequenceFile": "@file.path/@file.file"
		}
	    }
	}
    ]
}
