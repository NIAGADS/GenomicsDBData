    "preprocess": [
	{ 
	    "script" : {
		"skip": true,
		"name": "gunzip",
		"parameters": {
		    "none": "@file.path/@file.vcf+.gz"
		}
	    }
	},
	{
	    "script": 
	    { 
		"skip": true,
		"name": "runSnpEff",
		"parameters": {
		    "splitByChromosome": true,
		    "genomeBuild": "$GENOME_BUILD",
		    "vcf": "@file.vcf",
		    "filePath": "@file.path"
		}
	    }
	},
	{
	    "foreach": true,
	    "script": {
		"name": "vep",
		"parameters": {
		    "input_file": "foreach",
		    "output_file": "foreach",
		    "dir_cache": "@file.vep_cache",
		    "cache": true,
		    "offline": true,
		    "chr": "foreach",
		    "no_stats": true,
		    "json": true,
		    "fork": "4",
		    "sift": "b",
		    "polyphen": "b",
		    "ccds": true,
		    "symbol": true,
		    "numbers": true,
		    "domains": true,
		    "regulatory": true,
		    "canonical": true,
		    "protein": true,
		    "biotype": true,
		    "tsl": true,
		    "pubmed": true,
		    "uniprot": true,
		    "variant_class": true,
		    "exclude_predicted": true,
		    "gencode_basic": true,
		    "af": true,
		    "af_1kg": true,
		    "af_esp": true,
		    "af_gnomad": true,
		    "clin_sig_allele": 1,
		    "nearest": "gene",
		    "gene_phenotype": true,
		    "plugin": ["TSSDistance", "ExAC,@file.exac/ExAC.r0.3.sites.vep.vcf.gz"]
		}
	    }
	},
	{ 
	    "script" : {
		"skip": true,
		"name": "gzip",
		"parameters": {
		    "none" : "@file.path/@file.vcf"
		}
	    }
	},
	{
	    "plugin": {
		"skip": true,
		"comment": "OBSOLETE -- now loaded in AnnotatedVDB",
		"name": "GenomicsDBData::Load::Plugin::LoadVariantsFromVcf",
		"parameters": {
		    "vcfFileDir": "@file.path",
		    "variantSource": "dbSNP b151",
		    "extDbRlsSpec": "@external_database.name|@external_database.version",
		    "soExtDbRlsSpec": "Sequence Ontology|3.1",
		    "preprocess": true,
		    "vcfFile": "00-All.1.vcf,00-All.2.vcf,00-All.3.vcf,00-All.4.vcf,00-All.5.vcf,00-All.6.vcf,00-All.7.vcf,00-All.8.vcf,00-All.9.vcf,00-All.10.vcf,00-All.11.vcf,00-All.12.vcf,00-All.13.vcf,00-All.14.vcf,00-All.15.vcf,00-All.16.vcf,00-All.17.vcf,00-All.18.vcf,00-All.19.vcf,00-All.20.vcf,00-All.21.vcf,00-All.22.vcf,00-All.MT.vcf,00-All.X.vcf,00-All.Y.vcf"
		}
	    }
	}
    ],
    "load": [
	{
	    "plugin": {
		"skip": true,
		"comment": "OBSOLETE -- now loaded in AnnotatedVDB",
		"name": "GenomicsDBData::Load::Plugin::LoadVariantsFromVcf",
		"parameters": {
		    "vcfFileDir": "@file.path",
		    "variantSource": "dbSNP b151",
		    "extDbRlsSpec": "@external_database.name|@external_database.version",
		    "soExtDbRlsSpec": "Sequence Ontology|3.1",
		    "bulkLoad": true,
		    "vcfFile": "00-All.1.vcf,00-All.2.vcf,00-All.3.vcf,00-All.4.vcf,00-All.5.vcf,00-All.6.vcf,00-All.7.vcf,00-All.8.vcf,00-All.9.vcf,00-All.10.vcf,00-All.11.vcf,00-All.12.vcf,00-All.13.vcf,00-All.14.vcf,00-All.15.vcf,00-All.16.vcf,00-All.17.vcf,00-All.18.vcf,00-All.19.vcf,00-All.20.vcf,00-All.21.vcf,00-All.22.vcf,00-All.MT.vcf,00-All.X.vcf,00-All.Y.vcf"
		}
	    }
	}
    ],
    
    "tuning": ["$GUS_HOME/lib/sql/tuning_indexes/niagadsVariantIndexes.sql"]
}
